page{
    background-color: #fff;
}
.list{
    position: relative;
    min-height: 100px;
}
.progressBar{
    position: absolute;
    width: 1px;
    background-color: #F19584;
    top:25px;
    left:30px;
    opacity: 0.5;
    bottom: 20px;
}
.list-item{
    position: relative;
    margin-left: 60px;
    margin-top: 15px;
}
.point{
    position: absolute;
    width: 8px;
    height: 8px;
    background-color: #F19584;
    border-radius: 4px;
    top:42%;
    left:-33px;
}
.list-item-head{
    height: 40px;
    line-height: 40px;
}
.list-item-head-time{
    font-size: 13px;
    color: #aaa;
    margin-right: 20px;
}
.list-item-content{
    font-size: 13px;
    color:#aaa;
}
.footer .list-item-head-name{
    color:#F19584;
}
.footer .point{
    width: 26px;
    height: 26px;
    left:-42px;
    top:22%;
    border-radius: 13px;
    /*background-image: url("../../images/face.png");
    background-repeat: no-repeat;
    background-size: 100%;*/
    background-color: #fff;
}
.point image{
    height: 100%;
    width: 100%;
}

.panel{
    background-color: #eee;
    font-size: 15px;
}
.panel-title{
    height: 45px;
    line-height: 45px;
    padding-left:15px; 
}
.panel-content{
    background-color: #fff;
}
.panel-content-item{
    line-height: 45px;
    margin-left: 15px;
    margin-right: 15px;
    border-bottom: 1px solid #eee;
}
.panel-content-item-name{
    color: #aaa;
}
.panel-content-item-content{
    margin-left: 15px;
}
.panel-content .footer{
    border-bottom: 0;
}

.business{
    width: 100%;
    height: 45px;
    line-height: 45px;
    border-bottom: 1px solid #eee;
}
.business-name{
    margin-left: 10px;
    color: #aaa;
}
.business-img{
    float: left;
    margin-left:15px; 
    margin-top: 12px;
    height: 20px;
    width: 20px;
}
.business-img image{
    float: left;
    width: 100%;
    height: 100%;
}
.arrow-right{
    float:right;
    margin-right: 15px;
    margin-top: 15px;
    width: 12px;
    height: 12px;
}
.arrow-right image{
    float:right;
    width: 100%;
    height: 100%;
}
.body{
    min-height: 10px;
}
.body-item{
    padding: 5px 0;
}
.body-item text{
    margin:0 18px;
    color:#666;
}
.count{
    height: 45px;
    line-height: 45px;
    border-top:1px solid #eee;
    color:#666;
    margin-left: 15px;
    margin-right: 15px;
    margin-top: 10px;
}
.count text{
    font-size: 16px;
    color:#F19584;
}